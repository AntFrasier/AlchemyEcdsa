{
   "version":2,
   "builds": [
    {
        "src":"/api/*.js",
        "use":"@vercel/node"
    },
    {
        "src":"/client/*.html",
        "use":"@vercel/static"
    }
   ],

   "routes":[
    {
        "src":"/api/(.*)",
        "dest":"/api"
    },
    {
        "src":"/client/(.*)",
        "dest":"/client/.index.html"
    }

   ]
  }